{
  "version": 3,
  "configurePresets": [
    {
      "name": "gcc14",
      "displayName": "gcc14",
      "binaryDir": "${sourceDir}/build",
      "cmakeExecutable": "/snap/bin/cmake",
      "cacheVariables": {
        "CMAKE_C_COMPILER": "gcc-14",
        "CMAKE_CXX_COMPILER": "g++-14",
        "CMAKE_CXX_STANDARD": "23"
      }
    }
  ]
}
